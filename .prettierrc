{
  "semi": true,
  "trailingComma": "es5",
  "tabWidth": 4,
  "bracketSpacing": true,
  "singleQuote": false,
  "arrowParens": "always",
  "quoteProps": "consistent",
  "printWidth": 120,
  "plugins": ["prettier-plugin-tailwindcss", "prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    }
  ]
}
